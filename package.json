{
  "name": "software-engineering-project",
  "version": "0.2.0",
  "private": true,
  "main": "main/main.js",
  "description": "Electron + NextJS",
  "scripts": {
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron .\"",
    "build": "next build && electron-builder",
    "start": "next start",
    "lint": "next lint"
  },
  "build": {
    "extraResources": [
      {
        "from": "node_modules/.prisma/client/",
        "to": "app/node_modules/.prisma/client/"
      },
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/client/**/*"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.17",
    "@prisma/client": "^5.9.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.1",
    "crypto-js": "^4.2.0",
    "electron-is-dev": "^3.0.1",
    "electron-serve": "^1.2.0",
    "js-sha256": "^0.11.0",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "sweetalert2": "^11.10.4",
    "sweetalert2-react-content": "^5.0.7"
  },
  "devDependencies": {
    "@iconify-icons/icons8": "^1.2.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
